execute pathogen#infect()
color fu

set ruler
syntax on
filetype plugin indent on
set tabstop=4
set autoindent
set number
set shiftwidth=4
set shiftround
set showmatch
"set ignorecase
set smartcase
set hlsearch
set incsearch
set history=667
set undolevels=667
set pastetoggle=<F2>

highlight LineNr ctermfg=lightyellow 
